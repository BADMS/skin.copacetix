		</control>
	</include>
	<!-- Options background image -->
	<include name="OptionsBackgroundImage">
		<control type="image">
			<include>FullscreenDimensions</include>
			<fadetime>400</fadetime>
			<texture border="20">dialogs/OptionsBackground.png</texture>
		</control>
	</include>
	<!-- Clock -->
	<include name="Clock">
		<control type="grouplist">
			<right>50</right>
			<top>42</top>
			<width>400</width>
			<height>60</height>
